public Boolean isPrime(Integer num) {
    //code here
    boolean flag = true;
    // Check for factors
    for (Integer i = 2; i <= num / 2; i++) {
      if (math.mod(num,i) == 0) {
        //return false;
        flag = false;
        break;
      }
    }
      
    //return true;
    return flag;
}