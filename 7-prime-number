public Boolean isPrime(Integer num) {
    //code here
    boolean flag = true;
      
    // Check if number is <= 1
    if (n <= 1) {
      flag = false;
      return flag;
    }
      
    // Check for factors
    for (int i = 2; i <= n / 2; i++) {
      if (math.mod(n,i) == 0) {
        flag = false;
        break;
      }
    }
      
    return flag;
}