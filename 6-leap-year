public Boolean isLeapYear(Integer year) {
    //code here
    /* sol 1
    if( (math.mod(year,4)==0 && math.mod(year,100)!=0 ) || 
         math.mod(year,400)==0) {
        return true;
    }
    return false; */ 

    // sol 2
    if (math.mod(year,4) == 0) {
        if (math.mod(year,100) != 0) {
            return true;
        }
        return false;
    }
    else if (math.mod(year,400) == 0) {
        return true;
    }
    return false;
}
