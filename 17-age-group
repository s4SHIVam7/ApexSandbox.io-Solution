public String ageGroup(Integer n) {
    //code here
    Integer age = n;
    if (age<=1) 
    {
        return 'Infant';
    } 
    else if(age>=2 || age<=14) 
    {
        return 'Child';
    } 
    else if(age>=15 || age<=21) 
    {
        return 'Youth'''
    } 
    else if(age>=22) 
    {
        return 'Adult';
    } 
    return 'Error';
}