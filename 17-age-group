public String ageGroup(Integer n) {
    //code here
    if (n<=1) 
    {
        return 'Infant';
    } 
    else if(n>=2 || n<=14) 
    {
        return 'Child';
    } 
    else if(n>=15 || n<=21) 
    {
        return 'Youth'''
    } 
    else if(n>=22) 
    {
        return 'Adult';
    } //return 'Error';
}